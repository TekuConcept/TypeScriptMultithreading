{
  "name": "@tekuconcept/multithreaded",
  "version": "1.1.0",
  "description": "Simple multithreaded tasks in NodeJS using TypeScript",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/TekuConcept/TypeScriptMultithreading"
  },
  "homepage": "https://github.com/TekuConcept/TypeScriptMultithreading#readme",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist",
    "docs",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run write:cjs-package && npm run normalize:fileinfo",
    "write:cjs-package": "node scripts/write-cjs-package.mjs",
    "normalize:fileinfo": "node scripts/normalize-fileinfo.mjs",
    "prepublishOnly": "npm run build",
    "examples": "npm run build && node --loader ts-node/esm examples/index.ts"
  },
  "keywords": [
    "typescript",
    "multithreading",
    "worker",
    "nodejs"
  ],
  "author": "Chris Walker (TekuConcept)",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "copyfiles": "^2.4.1",
    "rimraf": "^6.1.2",
    "ts-node": "^10.9.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0"
  }
}
